#include<iostream.h>
int Days = 100; //100 Days of code
int main()
{
  while(Days >= 0){ //while it's not the 1st january
    cout <<"challenge Mode">>; //write "challenge mode"
    Days--; //decrement Days By one
}
    cout <<"Time For Another Challenge">>; //when the challenge ends there will be time for another one
}
